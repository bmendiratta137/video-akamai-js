{
  "name": "newrelic-video-akamai",
  "version": "0.3.0",
  "description": "New relic tracker for Akamai Media Player",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack -p",
    "build:dev": "webpack",
    "watch": "webpack -p --progress --color --watch",
    "watch:dev": "webpack --progress --color --watch",
    "prezip": "npm run build",
    "zip": "zip -P newrelic -x '*.DS_Store' -r amp.zip dist samples README.md CHANGELOG.md EULA.md",
    "clean": "rm -rf dist *.zip"
  },
  "repository": {
    "type": "git",
    "url": "git@source.datanerd.us:video/akamai-js.git"
  },
  "author": "Jordi Aguilar",
  "license": "UNLICENSED",
  "dependencies": {
    "newrelic-video-core": "git+https://github.com/newrelic/video-core-js.git#v0.9.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "webpack": "^3.4.1"
  }
}
